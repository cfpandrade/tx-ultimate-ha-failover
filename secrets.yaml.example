# secrets.yaml.example
# Copy this file to secrets.yaml and fill in your actual values
# NEVER commit secrets.yaml to git!

wifi_ssid: "YourWiFiSSID"
wifi_password: "YourWiFiPassword"

# Fallback access point password (when WiFi fails)
ap_pass: "FallbackPassword123"

# API encryption key (generate with: esphome config device.yaml)
api_key: "your-32-character-api-key-here"

# OTA update password
ota_pass: "your-ota-password-here"
